        StarterGui:SetCore("SendNotification", {
            Title="ÊàêÂäü",
            Text=("Â∑≤Â§çÂà∂ÊòµÁß∞: " .. targetPlayer.DisplayName),
            Duration=3
        })
    end
end)
local creds = window:Tab("ESPÈÄèËßÜ", "6031097229")
local credits = creds:section("ESP", true)
local playerESPEnabled = false;
local playerDistanceEnabled = false;
local playerNameEnabled = false;
local playerLineEnabled = false;
local playerHighlights = {}
local playerBillboards = {}
local playerLines = {}
local playerESPConnection = nil;
local playerAddedConnection = nil;
local playerRemovingConnection = nil;
local characterConnections = {}
local camera = workspace.CurrentCamera;
local function clearPlayerESP(player)
if playerHighlights[player] then
    playerHighlights[player]:Destroy()
    playerHighlights[player] = nil;
end
if playerBillboards[player] then
    playerBillboards[player]:Destroy()
    playerBillboards[player] = nil;
end
if playerLines[player] then
    if playerLines[player].Line then
        playerLines[player].Line:Remove()
        playerLines[player].Line = nil;
    end
    playerLines[player] = nil;
end
if characterConnections[player] then
    for _, connection in pairs(characterConnections[player]) do
        connection:Disconnect()
    end
    characterConnections[player] = nil;
end
end
local function clearAllESP()
local playersToClear = {}
for player, _ in pairs(playerHighlights) do
    playersToClear[player] = true;
end
for player, _ in pairs(playerBillboards) do
    playersToClear[player] = true;
end
for player, _ in pairs(playerLines) do
    playersToClear[player] = true;
end
for player, _ in pairs(playersToClear) do
    clearPlayerESP(player)
end
end
local function createPlayerESP(player)
if (player == game.Players.LocalPlayer) then
	return;
end
local character = player.Character;
if not character then
    return;
end
local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
if not humanoidRootPart then
    return;
end
clearPlayerESP(player)
if playerESPEnabled then
    local highlight = Instance.new("Highlight")
    highlight.Name = "PlayerHighlight"
    highlight.Adornee = character;
    highlight.FillColor = Color3.new(1, 1, 1)
    highlight.FillTransparency = 0.5;
    highlight.OutlineColor = Color3.new(0.8, 0.8, 0.8)
    highlight.OutlineTransparency = 0;
    highlight.Parent = character;
    playerHighlights[player] = highlight;
end
if (playerDistanceEnabled or playerNameEnabled) then
	local billboard = Instance.new("BillboardGui")
    billboard.Name = "PlayerInfo"
    billboard.Adornee = humanoidRootPart;
    billboard.Size = UDim2.new(0, 150, 0, 40)
    billboard.StudsOffset = Vector3.new(0, 2, 0)
	billboard.AlwaysOnTop = true;
    billboard.Parent = humanoidRootPart;
	if playerDistanceEnabled then
        local distanceText = Instance.new("TextLabel")
        distanceText.Name = "Distance"
        distanceText.BackgroundTransparency = 1;
		distanceText.Size = UDim2.new(1, 0, 0.5, 0)
        distanceText.Position = UDim2.new(0, 0, 0.5, 0)
        distanceText.Font = Enum.Font.GothamSemibold;
        distanceText.TextColor3 = Color3.new(1, 1, 1)
        distanceText.TextStrokeTransparency = 0;
		distanceText.TextStrokeColor3 = Color3.new(0, 0, 0)
        distanceText.TextSize = 14;
        distanceText.Text = ""
        distanceText.Parent = billboard;
    end
    if playerNameEnabled then
        local nameText = Instance.new("TextLabel")
        nameText.Name = "Name"
        nameText.BackgroundTransparency = 1;
        nameText.Size = UDim2.new(1, 0, 0.5, 0)
        nameText.Position = UDim2.new(0, 0, 0, 0)
        nameText.Font = Enum.Font.GothamBold;
        nameText.TextColor3 = Color3.new(1, 1, 1)
        nameText.TextStrokeTransparency = 0;
        nameText.TextStrokeColor3 = Color3.new(0, 0, 0)
		nameText.TextSize = 14;
        nameText.Text = player.Name;
        nameText.Parent = billboard;
    end
    playerBillboards[player] = billboard;
end
if playerLineEnabled then
    local line = Drawing.new("Line")
    line.Visible = true;
    line.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y)
	line.To = Vector2.new(0, 0)
    line.Color = Color3.new(1, 1, 1)
    line.Thickness = 1;
    line.Transparency = 1;
    playerLines[player] = {Line=line,TargetPart=humanoidRootPart}
end
if not characterConnections[player] then
    characterConnections[player] = {}
end
local humanoid = character:FindFirstChild("Humanoid")
if humanoid then
    table.insert(characterConnections[player], humanoid.Died:Connect(function()
	clearPlayerESP(player)
end))
end
table.insert(characterConnections[player], character.AncestryChanged:Connect(function()
if not character.Parent then
    clearPlayerESP(player)
end
end))
end
local function setupPlayerConnections()
if playerAddedConnection then
    playerAddedConnection:Disconnect()
end
if playerRemovingConnection then
    playerRemovingConnection:Disconnect()
end
playerAddedConnection = game.Players.PlayerAdded:Connect(function(player)
player.CharacterAdded:Connect(function(character)
wait(0.1)
if (playerESPEnabled or playerDistanceEnabled or playerNameEnabled or playerLineEnabled) then
    createPlayerESP(player)
end
end)
if player.Character then
    wait(0.1)
	if (playerESPEnabled or playerDistanceEnabled or playerNameEnabled or playerLineEnabled) then
        createPlayerESP(player)
    end
end
end)
playerRemovingConnection = game.Players.PlayerRemoving:Connect(function(player)
clearPlayerESP(player)
end)
for _, player in ipairs(game.Players:GetPlayers()) do
    if (player ~= game.Players.LocalPlayer) then
        player.CharacterAdded:Connect(function(character)
		wait(0.1)
        if (playerESPEnabled or playerDistanceEnabled or playerNameEnabled or playerLineEnabled) then
            createPlayerESP(player)
        end
    end)
    if player.Character then
        wait(0.1)
		if (playerESPEnabled or playerDistanceEnabled or playerNameEnabled or playerLineEnabled) then
			createPlayerESP(player)
        end
	end
end
end
end
local function updateAllPlayerESP()
clearAllESP()
if (not playerESPEnabled and not playerDistanceEnabled and not playerNameEnabled and not playerLineEnabled) then
    if playerESPConnection then
        playerESPConnection:Disconnect()
        playerESPConnection = nil;
    end
    return;
end
for _, player in ipairs(game.Players:GetPlayers()) do
    if ((player ~= game.Players.LocalPlayer) and player.Character) then
        createPlayerESP(player)
	end
end
setupPlayerConnections()
if not playerESPConnection then
	playerESPConnection = game:GetService("RunService").RenderStepped:Connect(function()
    updatePlayerInfo()
end)
end
end
function updatePlayerInfo()
local localPlayer = game.Players.LocalPlayer;
local localCharacter = localPlayer.Character;
if not localCharacter then
    return;
end
local localRoot = localCharacter:FindFirstChild("HumanoidRootPart")
if not localRoot then
    return;
end
for player, billboard in pairs(playerBillboards) do
    if (player.Character and player.Character:FindFirstChild("HumanoidRootPart")) then
		local humanoidRootPart = player.Character.HumanoidRootPart;
		if playerDistanceEnabled then
            local distanceLabel = billboard:FindFirstChild("Distance")
            if distanceLabel then
                local distance = (humanoidRootPart.Position - localRoot.Position).Magnitude;
				distanceLabel.Text = string.format("%.1fÁ±≥", distance)
            end
        end
        if playerNameEnabled then
            local nameLabel = billboard:FindFirstChild("Name")
            if nameLabel then
                nameLabel.Text = player.Name;
            end
        end
    end
end
for player, lineInfo in pairs(playerLines) do
    if (player.Character and player.Character:FindFirstChild("HumanoidRootPart") and lineInfo.Line) then
        local line = lineInfo.Line;
        local targetPart = player.Character.HumanoidRootPart;
        local worldPosition = targetPart.Position + Vector3.new(0, 1.5, 0)
        local screenPosition, onScreen = camera:WorldToViewportPoint(worldPosition)
        if onScreen then
            line.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y)
            line.To = Vector2.new(screenPosition.X, screenPosition.Y)
            line.Visible = true;
        else
		line.Visible = false;
    end
elseif (lineInfo and lineInfo.Line) then
	lineInfo.Line.Visible = false;
end
end
end
local function toggleESPFeature()
if (not playerESPEnabled and not playerDistanceEnabled and not playerNameEnabled and not playerLineEnabled) then
    if playerESPConnection then
		playerESPConnection:Disconnect()
        playerESPConnection = nil;
    end
	clearAllESP()
else
updateAllPlayerESP()
end
end
credits:Toggle("Áé©ÂÆ∂È´ò‰∫Æ", "PlayerHighlight", false, function(state)
playerESPEnabled = state;
toggleESPFeature()
end)
credits:Toggle("ÊòæÁ§∫Áé©ÂÆ∂Ë∑ùÁ¶ª", "PlayerDistance", false, function(state)
playerDistanceEnabled = state;
toggleESPFeature()
end)
credits:Toggle("ÊòæÁ§∫Áé©ÂÆ∂Áî®Êà∑Âêç", "PlayerName", false, function(state)
playerNameEnabled = state;
toggleESPFeature()
end)
credits:Toggle("Áé©ÂÆ∂ÊåáÂºïÁ∫ø", "PlayerLine", false, function(state)
playerLineEnabled = state;
toggleESPFeature()
end)

