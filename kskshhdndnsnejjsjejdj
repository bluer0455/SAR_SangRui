local creds = window:Tab("‰∫∫Áâ©Â±ûÊÄß", "6031097229")
local credits = creds:section("‰∫∫Áâ©Â±ûÊÄß", true)
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer;
local lockedProperties = {WalkSpeed=false,JumpPower=false,Gravity=false,HipHeight=false,CameraMaxZoomDistance=false,FieldOfView=false}
local propertyConnections = {}
local function getDefaultValues()
local defaults = {}
local player = LocalPlayer;
if (player.Character and player.Character:FindFirstChild("Humanoid")) then
    defaults.WalkSpeed = player.Character.Humanoid.WalkSpeed;
    defaults.JumpPower = player.Character.Humanoid.JumpPower;
    defaults.HipHeight = player.Character.Humanoid.HipHeight;
else
defaults.WalkSpeed = 16;
defaults.JumpPower = 50;
defaults.HipHeight = 2;
end
defaults.Gravity = workspace.Gravity;
defaults.CameraMaxZoomDistance = player.CameraMaxZoomDistance;
defaults.FieldOfView = workspace.CurrentCamera.FieldOfView;
return defaults;
end
local defaultValues = getDefaultValues()
local playerSettings = {WalkSpeed=defaultValues.WalkSpeed,JumpPower=defaultValues.JumpPower,Gravity=defaultValues.Gravity,HipHeight=defaultValues.HipHeight,CameraMaxZoomDistance=defaultValues.CameraMaxZoomDistance,FieldOfView=defaultValues.FieldOfView}
local function updatePlayerAttributes()
local player = LocalPlayer;
if (player.Character and player.Character:FindFirstChild("Humanoid")) then
    local humanoid = player.Character.Humanoid;
	if not lockedProperties.WalkSpeed then
        humanoid.WalkSpeed = math.min(playerSettings.WalkSpeed, 500)
    else
    humanoid.WalkSpeed = playerSettings.WalkSpeed;
end
if not lockedProperties.JumpPower then
    humanoid.JumpPower = math.min(playerSettings.JumpPower, 500)
else
humanoid.JumpPower = playerSettings.JumpPower;
end
if not lockedProperties.HipHeight then
    humanoid.HipHeight = math.min(playerSettings.HipHeight, 500)
else
humanoid.HipHeight = playerSettings.HipHeight;
end
end
if not lockedProperties.Gravity then
	workspace.Gravity = playerSettings.Gravity;
end
if not lockedProperties.CameraMaxZoomDistance then
    player.CameraMaxZoomDistance = playerSettings.CameraMaxZoomDistance;
end
if not lockedProperties.FieldOfView then
	workspace.CurrentCamera.FieldOfView = math.min(playerSettings.FieldOfView, 120)
end
end
local function lockProperty(propertyName, state)
lockedProperties[propertyName] = state;
if propertyConnections[propertyName] then
    propertyConnections[propertyName]:Disconnect()
    propertyConnections[propertyName] = nil;
end
if state then
    if ((propertyName == "WalkSpeed") or (propertyName == "JumpPower") or (propertyName == "HipHeight")) then
        local function setupHumanoidConnection(humanoid)
        if not humanoid then
            return;
        end
        humanoid[propertyName] = playerSettings[propertyName]
        propertyConnections[propertyName] = humanoid:GetPropertyChangedSignal(propertyName):Connect(function()
		if lockedProperties[propertyName] then
			humanoid[propertyName] = playerSettings[propertyName]
		end
    end)
end
if LocalPlayer.Character then
    local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
	setupHumanoidConnection(humanoid)
end
LocalPlayer.CharacterAdded:Connect(function(character)
local humanoid = character:WaitForChild("Humanoid")
setupHumanoidConnection(humanoid)
end)
elseif (propertyName == "Gravity") then
	workspace.Gravity = playerSettings.Gravity;
    propertyConnections[propertyName] = workspace:GetPropertyChangedSignal("Gravity"):Connect(function()
    if lockedProperties.Gravity then
        workspace.Gravity = playerSettings.Gravity;
    end
end)
elseif (propertyName == "CameraMaxZoomDistance") then
    LocalPlayer.CameraMaxZoomDistance = playerSettings.CameraMaxZoomDistance;
    propertyConnections[propertyName] = LocalPlayer:GetPropertyChangedSignal("CameraMaxZoomDistance"):Connect(function()
    if lockedProperties.CameraMaxZoomDistance then
        LocalPlayer.CameraMaxZoomDistance = playerSettings.CameraMaxZoomDistance;
    end
end)
elseif (propertyName == "FieldOfView") then
    workspace.CurrentCamera.FieldOfView = playerSettings.FieldOfView;
    propertyConnections[propertyName] = workspace.CurrentCamera:GetPropertyChangedSignal("FieldOfView"):Connect(function()
    if lockedProperties.FieldOfView then
        workspace.CurrentCamera.FieldOfView = playerSettings.FieldOfView;
	end
end)
end
end
end
LocalPlayer.CharacterAdded:Connect(function(character)
local humanoid = character:WaitForChild("Humanoid")
if humanoid then
    if lockedProperties.WalkSpeed then
        humanoid.WalkSpeed = playerSettings.WalkSpeed;
    else
    humanoid.WalkSpeed = math.min(playerSettings.WalkSpeed, 500)
end
if lockedProperties.JumpPower then
    humanoid.JumpPower = playerSettings.JumpPower;
else
humanoid.JumpPower = math.min(playerSettings.JumpPower, 500)
end
if lockedProperties.HipHeight then
    humanoid.HipHeight = playerSettings.HipHeight;
else
humanoid.HipHeight = math.min(playerSettings.HipHeight, 500)
end
if lockedProperties.WalkSpeed then
	lockProperty("WalkSpeed", true)
end
if lockedProperties.JumpPower then
    lockProperty("JumpPower", true)
end
if lockedProperties.HipHeight then
	lockProperty("HipHeight", true)
end
end
end)
credits:Slider("ÈÄüÂ∫¶ (ÈªòËÆ§:" .. defaultValues.WalkSpeed .. ")", "WalkspeedSlider", defaultValues.WalkSpeed, 0, 500, false, function(Value)
playerSettings.WalkSpeed = Value;
updatePlayerAttributes()
end)
local speedMultiplier = 1;
local velocityConnection = nil;
credits:Slider("ÁßªÂä®ÈÄüÂ∫¶2", "SpeedMultiplier", 0, 0, 500, false, function(value)
speedMultiplier = value / 100;
if (not velocityConnection and (speedMultiplier > 1)) then
    velocityConnection = game:GetService("RunService").Heartbeat:Connect(function()
    local player = game.Players.LocalPlayer;
    local character = player.Character;
    if (character and character:FindFirstChild("Humanoid") and character:FindFirstChild("HumanoidRootPart")) then
        local humanoid = character.Humanoid;
        local rootPart = character.HumanoidRootPart;
        local moveDirection = humanoid.MoveDirection;
        if (moveDirection.Magnitude > 0) then
            local currentVelocity = rootPart.Velocity;
            local newVelocity = moveDirection * humanoid.WalkSpeed * speedMultiplier * 1.5;
			newVelocity = Vector3.new(newVelocity.X, currentVelocity.Y, newVelocity.Z)
            rootPart.Velocity = newVelocity;
        end
	end
end)
end
if (velocityConnection and (speedMultiplier <= 1)) then
    velocityConnection:Disconnect()
	velocityConnection = nil;
end
end)
credits:Toggle("ÈîÅÂÆöÈÄüÂ∫¶", "", false, function(state)
lockProperty("WalkSpeed", state)
end)
