local creds = window:Tab("Áé©ÂÆ∂ÈÄâÊã©ÂíåËá™ÁûÑ", "6031097229")
local credits = creds:section("Áé©ÂÆ∂ÈÄâÊã©", true)
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local Cam = workspace.CurrentCamera;
local LocalPlayer = Players.LocalPlayer;
local aimbotEnabled = false;
local fovRadius = 30;
local smoothFactor = 1;
local currentTarget = "all"
local targetPlayer = nil;
local targetDropdown;
local FOVring = Drawing.new("Circle")
FOVring.Visible = false;
FOVring.Thickness = 2;
FOVring.Color = Color3.fromRGB(231, 231, 236)
FOVring.Filled = false;
FOVring.Radius = fovRadius;
FOVring.Position = Cam.ViewportSize / 2;
local spectateConnection = nil;
local defaultCameraType = Cam.CameraType;
local defaultCameraSubject = Cam.CameraSubject;
local spectating = false;
local function updateDrawings()
FOVring.Position = Cam.ViewportSize / 2;
end
local function getClosestPlayerInFOV(trg_part)
local nearest = nil;
local last = math.huge;
local playerMousePos = Cam.ViewportSize / 2;
local targetList = {}
if (currentTarget == "all") then
    for _, player in ipairs(Players:GetPlayers()) do
        if (player ~= LocalPlayer) then
            table.insert(targetList, player)
        end
	end
elseif (targetPlayer and targetPlayer.Character) then
    table.insert(targetList, targetPlayer)
end
for _, player in ipairs(targetList) do
    if player.Character then
		local part = player.Character:FindFirstChild(trg_part)
        if part then
            local ePos, isVisible = Cam:WorldToViewportPoint(part.Position)
			local distance = (Vector2.new(ePos.x, ePos.y) - playerMousePos).Magnitude;
            if ((distance < last) and isVisible and (distance < fovRadius)) then
				last = distance;
                nearest = player;
            end
        end
	end
end
return nearest;
end
local function updatePlayerList()
local options = {"ÊâÄÊúâÁé©ÂÆ∂"}
for _, player in ipairs(Players:GetPlayers()) do
    if (player ~= LocalPlayer) then
        local name = player.Name;
        if (player.DisplayName ~= name) then
            name = name .. "(" .. player.DisplayName .. ")"
        end
        table.insert(options, name)
    end
end
if targetDropdown then
    targetDropdown:SetOptions(options)
    if ((currentTarget == "single") and (not targetPlayer or not Players:FindFirstChild(targetPlayer.Name))) then
		currentTarget = "all"
		targetPlayer = nil;
        targetDropdown:SetTitle("ÁõÆÊ†á: ÊâÄÊúâÁé©ÂÆ∂")
    end
end
end
targetDropdown = credits:Dropdown("ÈÄâÊã©ÁõÆÊ†áÁé©ÂÆ∂", "ÁÇπÂáªÈÄâÊã©ÁõÆÊ†á", {"ÊâÄÊúâÁé©ÂÆ∂"}, function(selected)
if (selected == "ÊâÄÊúâÁé©ÂÆ∂") then
    currentTarget = "all"
    targetPlayer = nil;
	targetDropdown:SetTitle("ÁõÆÊ†á: ÊâÄÊúâÁé©ÂÆ∂")
else
local username = selected:match("^(.-)%(") or selected;
username = username:gsub("%s+$", "")
for _, player in ipairs(Players:GetPlayers()) do
	if (player.Name == username) then
        currentTarget = "single"
        targetPlayer = player;
        targetDropdown:SetTitle("ÁõÆÊ†á: " .. player.Name .. (((player.DisplayName ~= player.Name) and ("(" .. player.DisplayName .. ")")) or ""))
        break;
    end
end
end
end)
updatePlayerList()
Players.PlayerAdded:Connect(function()
task.wait(1)
updatePlayerList()
end)
Players.PlayerRemoving:Connect(function()
task.wait(0.5)
updatePlayerList()
end)
local aimbotConnection;
credits:Toggle("ÂºÄÂêØËá™ÁûÑ", "AimbotToggle", false, function(state)
aimbotEnabled = state;
FOVring.Visible = state;
if state then
    aimbotConnection = RunService.RenderStepped:Connect(function()
    updateDrawings()
    local closest = getClosestPlayerInFOV("Head")
    if (closest and closest.Character and closest.Character:FindFirstChild("Head")) then
        local targetPosition = closest.Character.Head.Position;
        local currentCFrame = Cam.CFrame;
        local targetCFrame = CFrame.new(currentCFrame.Position, targetPosition)
        if (smoothFactor >= 4.99) then
            Cam.CFrame = targetCFrame;
        else
        Cam.CFrame = currentCFrame:Lerp(targetCFrame, math.min(smoothFactor, 1))
    end
end
end)
elseif aimbotConnection then
    aimbotConnection:Disconnect()
    aimbotConnection = nil;
end
end)
credits:Slider("Ëá™ÁûÑFOVÂ§ßÂ∞è", "FOVSizeSlider", fovRadius, 10, 500, false, function(value)
fovRadius = value;
FOVring.Radius = value;
end)
credits:Slider("Ëá™ÁûÑÂº∫Â∫¶", "AimbotSmoothSlider", 100, 1, 500, false, function(value)
smoothFactor = math.min(value / 100, 5)
end)
credits:Button("‰º†ÈÄÅÂà∞ÁõÆÊ†áÁé©ÂÆ∂", function()
if (currentTarget == "all") then
    StarterGui:SetCore("SendNotification", {Title="ÈîôËØØ",Text="ÂΩìÂâçÊåâÈíÆ‰∏çÊîØÊåÅÊâÄÊúâ",Duration=3})
    return;
end
if (targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart")) then
    local hrp = targetPlayer.Character.HumanoidRootPart;
    local char = LocalPlayer.Character;
    if (char and char:FindFirstChild("HumanoidRootPart")) then
        char.HumanoidRootPart.CFrame = hrp.CFrame + Vector3.new(0, 5, 0)
    end
end
end)
credits:Toggle("ÁõëËßÜÁõÆÊ†áÁé©ÂÆ∂ËßÜËßí", "SpectateToggle", false, function(state)
if (currentTarget == "all") then
	StarterGui:SetCore("SendNotification", {Title="ÈîôËØØ",Text="ÂΩìÂâçÊåâÈíÆ‰∏çÊîØÊåÅÊâÄÊúâ",Duration=3})
    return;
end
spectating = state;
if state then
    if (targetPlayer and targetPlayer.Character) then
        defaultCameraType = Cam.CameraType;
        defaultCameraSubject = Cam.CameraSubject;
        Cam.CameraType = Enum.CameraType.Custom;
        Cam.CameraSubject = targetPlayer.Character:WaitForChild("Humanoid", 5)
        if Cam.CameraSubject then
            local distance = 10;
            local angle = 0;
            spectateConnection = RunService.RenderStepped:Connect(function()
            if not spectating then
                return;
            end
            if (not targetPlayer or not targetPlayer.Character) then
                spectateConnection:Disconnect()
                return;
            end
            local rootPart = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            if not rootPart then
                return;
            end
            if (LocalPlayer:GetMouse().WheelForward > 0) then
                distance = math.max(1, distance - 1)
            elseif (LocalPlayer:GetMouse().WheelBackward > 0) then
                distance = math.min(50, distance + 1)
            end
            local offset = Vector3.new(math.sin(angle) * distance, 3, math.cos(angle) * distance)
            Cam.CFrame = CFrame.new(rootPart.Position + offset, rootPart.Position + Vector3.new(0, 2, 0))
        end)
    end
end
else
if spectateConnection then
    spectateConnection:Disconnect()
    spectateConnection = nil;
end
Cam.CameraType = defaultCameraType;
Cam.CameraSubject = defaultCameraSubject;
end
end)
credits:Button("Â§çÂà∂Áé©ÂÆ∂Áî®Êà∑Âêç", function()
    if (currentTarget == "all") then
        StarterGui:SetCore("SendNotification", {Title="ÈîôËØØ",Text="ËØ∑ÂÖàÈÄâÊã©ÁâπÂÆöÁé©ÂÆ∂",Duration=3})
        return
    end
    
    if targetPlayer then
        setclipboard(targetPlayer.Name)
        StarterGui:SetCore("SendNotification", {
            Title="ÊàêÂäü",
            Text=("Â∑≤Â§çÂà∂Áî®Êà∑Âêç: " .. targetPlayer.Name),
            Duration=3
        })
    end
end)

credits:Button("Â§çÂà∂Áé©ÂÆ∂ÊòµÁß∞", function()
    if (currentTarget == "all") then
        StarterGui:SetCore("SendNotification", {Title="ÈîôËØØ",Text="ËØ∑ÂÖàÈÄâÊã©ÁâπÂÆöÁé©ÂÆ∂",Duration=3})
        return
    end
    
