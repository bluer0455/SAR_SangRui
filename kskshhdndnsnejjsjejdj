    teleportDelay = value
end)

credits:Button("‰º†ÈÄÅÂà∞ËÆ∞ÂΩï‰ΩçÁΩÆ", function()
    if recordedPosition then
        local player = game.Players.LocalPlayer;
        local character = player.Character or player.CharacterAdded:Wait()
        
                game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "‰º†ÈÄÅ‰∏≠",
            Text = string.format("%.1fÁßíÂêé‰º†ÈÄÅ", teleportDelay),
            Duration = teleportDelay > 0 and teleportDelay or 1
        })
        
                wait(teleportDelay)
        
        if (character and character:FindFirstChild("HumanoidRootPart")) then
            character.HumanoidRootPart.CFrame = CFrame.new(recordedPosition)
        end
    else
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "ÈîôËØØ",
            Text = "Ê≤°ÊúâËÆ∞ÂΩïÁöÑ‰ΩçÁΩÆ",
            Duration = 2
        })
    end
end)
