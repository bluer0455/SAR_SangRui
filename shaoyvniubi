local creds = window:Tab("è‡ªç„¶ç¾å®³æ¨¡æ‹Ÿå™¨", "6031097229")
local credits = creds:section("è‡ªç„¶ç¾å®³æ¨¡æ‹Ÿå™¨", false)

credits:Label("é›ªå´©é¢„æµ‹ä¸äº†")
credits:Label("ç—…æ¯’ä¸çŸ¥é“")
credits:Button("åœ¨èŠå¤©æ è¾“å…¥ä¸‹ä¸€ä¸ªç¾éš¾", function()
    local Players = game:GetService("Players")
    local TextChatService = game:GetService("TextChatService")
    local plr = Players.LocalPlayer

    nextdis = true
    local lastDisaster = nil

    while wait(1) and nextdis do
        local SurvivalTag = plr.Character:FindFirstChild("SurvivalTag")
        
        if SurvivalTag then
            local disasterMessages = {
                Blizzard = "æš´é£é›ª",
                Sandstorm = "æ²™å°˜æš´",
                Tornado = "é¾™å·é£",
                ["Volcanic Eruption"] = "ç«å±±",
                ["Flash Flood"] = "æ´ªæ°´",
                ["Deadly Virus"] = "ç—…æ¯’",
                Tsunami = "æµ·å•¸",
                ["Acid Rain"] = "é…¸é›¨",
                Fire = "ç«ç„°",
                ["Meteor Shower"] = "æµæ˜Ÿé›¨",
                Earthquake = "åœ°éœ‡",
                ["Thunder Storm"] = "æš´é£é›¨"
            }
            
            local currentDisaster = SurvivalTag.Value
            local disasterName = disasterMessages[currentDisaster]
            
            if disasterName and currentDisaster ~= lastDisaster and nextdis then
                TextChatService.TextChannels.RBXGeneral:SendAsync("ä¸‹ä¸€ä¸ªç¾éš¾æ˜¯ï¼š" .. disasterName)
                lastDisaster = currentDisaster
            end
        end
    end
end)

credits:Button("å…å»å è½ä¼¤å®³", function()
    local runsvc = game:GetService("RunService")
    local heartbeat = runsvc.Heartbeat
    local rstepped = runsvc.RenderStepped

    local lp = game:GetService("Players").LocalPlayer

    local novel = Vector3.zero

    local function nofalldamage(chr)
        local root = chr:WaitForChild("HumanoidRootPart")
        
        if root then
            local con
            con = heartbeat:Connect(function()
                if not root.Parent then
                    con:Disconnect()
                end
                
                local oldvel = root.AssemblyLinearVelocity
                root.AssemblyLinearVelocity = novel
                
                rstepped:Wait()
                root.AssemblyLinearVelocity = oldvel
            end)
        end
    end

    if lp.Character then
        nofalldamage(lp.Character)
    end

    lp.CharacterAdded:Connect(nofalldamage)
end)

credits:Toggle("æ¸¸æˆå²›æ‚¬å´–ç¢°æ’", "Togglelnfo", false, function(bool)
    for i, v in pairs(game.workspace:GetDescendants()) do
        if v.Name == 'LowerRocks' then
            v.CanCollide = bool
        end
    end
end)

credits:Toggle("åœ¨æ°´ä¸Šè¡Œèµ°", "ToggleInfo", false, function(bool)
    if bool == false then 
        game.Workspace.WaterLevel.CanCollide = false
        game.Workspace.WaterLevel.Size = Vector3.new(10, 1, 10)
    end
    
    if bool == true then 
        game.Workspace.WaterLevel.CanCollide = true
        game.Workspace.WaterLevel.Size = Vector3.new(5000, 1, 5000)
    end
end)

