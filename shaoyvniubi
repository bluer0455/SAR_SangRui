local credits = creds:section("设置", true)
credits:Toggle("移除UI辉光", "", false, function(state)
    if state then
        game:GetService("CoreGui")["frosty is cute"].Main.DropShadowHolder.Visible = false
    else
        game:GetService("CoreGui")["frosty is cute"].Main.DropShadowHolder.Visible = true
    end
end)

credits:Toggle("显示时间", "", false, function(state)
    if state then
        TimeGui = Instance.new("ScreenGui")
        TimeLabel = Instance.new("TextLabel")
        
        TimeGui.Name = "TimeDisplay"
        TimeGui.Parent = game.CoreGui
        TimeGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
        
        TimeLabel.Parent = TimeGui
        TimeLabel.BackgroundTransparency = 1
        TimeLabel.Position = UDim2.new(0.75, 0, 0.010, 0)
        TimeLabel.Size = UDim2.new(0, 133, 0, 30)
        TimeLabel.Font = Enum.Font.GothamSemibold
        TimeLabel.TextColor3 = Color3.new(1, 1, 1)
        TimeLabel.TextScaled = true
        TimeLabel.TextWrapped = true
        
        HeartbeatConnection = game:GetService("RunService").Heartbeat:Connect(function()
            TimeLabel.Text = os.date("SAR:%H时%M分%S秒")
        end)
    else
        if HeartbeatConnection then
            HeartbeatConnection:Disconnect()
            HeartbeatConnection = nil
        end
        if TimeGui then
            TimeGui:Destroy()
            TimeGui = nil
        end
    end
end)

credits:Button("摧毁GUI",function()
    game:GetService("CoreGui")["frosty is cute"]:Destroy()
end)

local creds = window:Tab("属性", "6031097229")
local credits = creds:section("人物属性", true)

local lockedProperties = {
    WalkSpeed = false,
    JumpPower = false,
    Gravity = false,
    HipHeight = false,
    CameraMaxZoomDistance = false,
    FieldOfView = false
}

local function getDefaultValues()
    local defaults = {}
    local player = game.Players.LocalPlayer
    
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        defaults.WalkSpeed = player.Character.Humanoid.WalkSpeed
        defaults.JumpPower = player.Character.Humanoid.JumpPower
        defaults.HipHeight = player.Character.Humanoid.HipHeight
    else
        
        defaults.WalkSpeed = 16
        defaults.JumpPower = 50
        defaults.HipHeight = 2
    end
   
    defaults.Gravity = workspace.Gravity
    defaults.CameraMaxZoomDistance = player.CameraMaxZoomDistance
    defaults.FieldOfView = workspace.CurrentCamera.FieldOfView
    
    return defaults
end

local defaultValues = getDefaultValues()

local playerSettings = {
    WalkSpeed = defaultValues.WalkSpeed,
    JumpPower = defaultValues.JumpPower,
    Gravity = defaultValues.Gravity,
    HipHeight = defaultValues.HipHeight,
    CameraMaxZoomDistance = defaultValues.CameraMaxZoomDistance,
    FieldOfView = defaultValues.FieldOfView
}

local function updatePlayerAttributes()
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        if not lockedProperties.WalkSpeed then
            player.Character.Humanoid.WalkSpeed = math.min(playerSettings.WalkSpeed, 500)
        end
        if not lockedProperties.JumpPower then
            player.Character.Humanoid.JumpPower = math.min(playerSettings.JumpPower, 500)
        end
        if not lockedProperties.HipHeight then
            player.Character.Humanoid.HipHeight = math.min(playerSettings.HipHeight, 500)
        end
    end
    if not lockedProperties.Gravity then
        game.Workspace.Gravity = playerSettings.Gravity
    end
    if not lockedProperties.CameraMaxZoomDistance then
        player.CameraMaxZoomDistance = playerSettings.CameraMaxZoomDistance
    end
    if not lockedProperties.FieldOfView then
        game.Workspace.CurrentCamera.FieldOfView = math.min(playerSettings.FieldOfView, 120)
    end
end

local function lockProperty(propertyName, value)
    lockedProperties[propertyName] = value
    
    if value then
        if propertyName == "WalkSpeed" or propertyName == "JumpPower" or propertyName == "HipHeight" then
            local humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid:GetPropertyChangedSignal(propertyName):Connect(function()
                    if lockedProperties[propertyName] then
                        humanoid[propertyName] = playerSettings[propertyName]
                    end
                end)
            end
        elseif propertyName == "Gravity" then
            workspace:GetPropertyChangedSignal("Gravity"):Connect(function()
                if lockedProperties.Gravity then
                    workspace.Gravity = playerSettings.Gravity
                end
            end)
        elseif propertyName == "CameraMaxZoomDistance" then

            game.Players.LocalPlayer:GetPropertyChangedSignal("CameraMaxZoomDistance"):Connect(function()
                if lockedProperties.CameraMaxZoomDistance then
                    game.Players.LocalPlayer.CameraMaxZoomDistance = playerSettings.CameraMaxZoomDistance
                end
            end)
        elseif propertyName == "FieldOfView" then
            workspace.CurrentCamera:GetPropertyChangedSignal("FieldOfView"):Connect(function()
                if lockedProperties.FieldOfView then
                    workspace.CurrentCamera.FieldOfView = playerSettings.FieldOfView
                end
            end)
        end
    end
end

game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    local humanoid = character:WaitForChild("Humanoid")
    if humanoid then
        if not lockedProperties.WalkSpeed then
            humanoid.WalkSpeed = math.min(playerSettings.WalkSpeed, 500)
        end
        if not lockedProperties.JumpPower then
            humanoid.JumpPower = math.min(playerSettings.JumpPower, 500)
        end
        if not lockedProperties.HipHeight then
            humanoid.HipHeight = math.min(playerSettings.HipHeight, 500)
        end
        
        if lockedProperties.WalkSpeed then
            humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
                humanoid.WalkSpeed = playerSettings.WalkSpeed
            end)
        end
        if lockedProperties.JumpPower then
            humanoid:GetPropertyChangedSignal("JumpPower"):Connect(function()
                humanoid.JumpPower = playerSettings.JumpPower
            end)
        end
        if lockedProperties.HipHeight then
            humanoid:GetPropertyChangedSignal("HipHeight"):Connect(function()
                humanoid.HipHeight = playerSettings.HipHeight
            end)
        end
    end
end)

credits:Slider('速度 (默认:'..defaultValues.WalkSpeed..')', 'WalkspeedSlider', defaultValues.WalkSpeed, 0, 500, false, function(Value)
    playerSettings.WalkSpeed = Value
    updatePlayerAttributes()
end)
credits:Toggle("锁定速度", "", false, function(state)
    lockProperty("WalkSpeed", state)
end)

credits:Slider('跳跃高度 (默认:'..defaultValues.JumpPower..')', 'JumpPowerSlider', defaultValues.JumpPower, 0, 500, false, function(Value)
    playerSettings.JumpPower = Value
    updatePlayerAttributes()
end)
credits:Toggle("锁定跳跃高度", "", false, function(state)
    lockProperty("JumpPower", state)
end)

credits:Slider('重力 (默认:'..defaultValues.Gravity..')', 'GravitySlider', defaultValues.Gravity, 0, 1000, false, function(Value)
    playerSettings.Gravity = Value
    updatePlayerAttributes()
end)
credits:Toggle("锁定重力", "", false, function(state)
    lockProperty("Gravity", state)
end)

credits:Slider('高度 (默认:'..defaultValues.HipHeight..')', 'Slider', defaultValues.HipHeight, 0, 500, false, function(Value)
    playerSettings.HipHeight = Value
    updatePlayerAttributes()
end)
credits:Toggle("锁定高度", "", false, function(state)
    lockProperty("HipHeight", state)
end)

credits:Slider('最大缩放距离 (默认:'..defaultValues.CameraMaxZoomDistance..')', 'ZOOOOOM OUT!', defaultValues.CameraMaxZoomDistance, 0, 200000, false, function(Value)
    playerSettings.CameraMaxZoomDistance = Value
    updatePlayerAttributes()
end)
credits:Toggle("锁定缩放距离", "", false, function(state)
    lockProperty("CameraMaxZoomDistance", state)
end)

credits:Slider('超广角 (默认:'..defaultValues.FieldOfView..')', 'Sliderflag', defaultValues.FieldOfView, 0.1, 120, false, function(Value)
    playerSettings.FieldOfView = Value
    updatePlayerAttributes()
end)
credits:Toggle("锁定超广角", "", false, function(state)
    lockProperty("FieldOfView", state)
end)

local creds = window:Tab("通用", "6031097229")
local credits = creds:section("通用", true)

local lightingDefaults = {
    Ambient = game:GetService("Lighting").Ambient,
    Brightness = game:GetService("Lighting").Brightness,
    ColorShift_Bottom = game:GetService("Lighting").ColorShift_Bottom,
    ColorShift_Top = game:GetService("Lighting").ColorShift_Top,
    GlobalShadows = game:GetService("Lighting").GlobalShadows,
    ShadowSoftness = game:GetService("Lighting").ShadowSoftness,
    FogStart = game:GetService("Lighting").FogStart,
    FogEnd = game:GetService("Lighting").FogEnd,
    FogColor = game:GetService("Lighting").FogColor
}

credits:Toggle(
    "夜视", 
    "WhiteLightToggle", 
    false, 
    function(state)
        if state then
            lightingDefaults.Ambient = game:GetService("Lighting").Ambient
            lightingDefaults.Brightness = game:GetService("Lighting").Brightness
            game:GetService("Lighting").Ambient = Color3.fromRGB(255, 255, 255)
            game:GetService("Lighting").Brightness = 2
        else
            game:GetService("Lighting").Ambient = lightingDefaults.Ambient
            game:GetService("Lighting").Brightness = lightingDefaults.Brightness
        end
    end)

credits:Toggle(
    "移除阴影", 
    "NoShadowsToggle", 
    false, 
    function(state)
        if state then
            game:GetService("Lighting").GlobalShadows = false
            game:GetService("Lighting").ShadowSoftness = 0
        else
            game:GetService("Lighting").GlobalShadows = lightingDefaults.GlobalShadows
            game:GetService("Lighting").ShadowSoftness = lightingDefaults.ShadowSoftness
        end
    end)

credits:Toggle(
    "移除雾效果", 
    "NoFogToggle", 
    false, 
    function(state)
        if state then
            game:GetService("Lighting").FogStart = 100000
            game:GetService("Lighting").FogEnd = 100000
        else
            game:GetService("Lighting").FogStart = lightingDefaults.FogStart
            game:GetService("Lighting").FogEnd = lightingDefaults.FogEnd
            game:GetService("Lighting").FogColor = lightingDefaults.FogColor
        end
    end)

credits:Toggle(
    "穿墙", 
    "WallhackToggle", 
    false, 
    function(state)
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        
        if not library.flags.WallhackOriginal then
            library.flags.WallhackOriginal = {}
        end

        if state then
            for _, part in ipairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    library.flags.WallhackOriginal[part] = part.CanCollide
                    part.CanCollide = false
                end
            end
            
            library.flags.WallhackConnection = game:GetService("RunService").Heartbeat:Connect(function()
                for part, _ in pairs(library.flags.WallhackOriginal) do
                    if part and part.Parent then
                        part.CanCollide = false
                    end
                end
            end)
        else
            if library.flags.WallhackConnection then
                library.flags.WallhackConnection:Disconnect()
                library.flags.WallhackConnection = nil
            end
            
            for part, canCollide in pairs(library.flags.WallhackOriginal) do
                if part and part.Parent then
                    part.CanCollide = canCollide
                end
            end
            
            if humanoid and humanoid.FloorMaterial ~= Enum.Material.Air then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
            
            library.flags.WallhackOriginal = {}
        end
    end)
    credits:Toggle(
    "无限跳跃",
"InfiniteJumpToggle",
false,
function(state)
    local player = game.Players.LocalPlayer
    
    if state then
        library.flags.InfiniteJumpConnection = game:GetService("UserInputService").JumpRequest:Connect(function()
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    else
        if library.flags.InfiniteJumpConnection then
            library.flags.InfiniteJumpConnection:Disconnect()
            library.flags.InfiniteJumpConnection = nil
        end
    end
end)

