}, function(selected)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local Animate = character:WaitForChild("Animate")
 
local runAnimationIDs = {
["吸血鬼动画包"] = "1083462077",
["宇航员动画包"] = "891636393",
["泡泡动画包"] = "910025107",
["卡通动画包"] = "742638842",
